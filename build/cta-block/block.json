      "type": "number",
      "default": 3000
    },
    "autoplay": {
      "type": "boolean",
      "default": true
    },
    "autoplayDelay": {
      "type": "number",
      "default": 4000
    },
    "blockId": {
      "type": "string",
      "default": ""
    }

    "itemPadding": {
      "type": "number",
      "default": 32
    },
    "gap": {
      "type": "number",
      "default": 1
    },
    "containerMode": {
      "type": "string",
      "default": "full"
    },
    "containerMaxWidth": {
      "type": "object",
      "default": {
        "desktop": {
          "value": 1200,

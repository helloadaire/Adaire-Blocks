      "type": "string",
      "default": "#1a1a2e"
    },
    "hoverTaglineColor": {
      "type": "string",
      "default": "#1a1a2e"
    },
    "hoverDescriptionColor": {
      "type": "string",
      "default": "#1a1a2e"
    },
    "iconColor": {
      "type": "string",
      "default": "#8b7cf7"
    },
    "borderColor": {
      "type": "string",
      "default": "#2a2a4e"
    },
    "containerBackgroundColor": {
      "type": "string",
      "default": "#2a2a4e"
    },

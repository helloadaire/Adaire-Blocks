{
  "apiVersion": 3,
  "name": "create-block/container-block",
  "version": "1.0.0",
  "title": "Container",
  "category": "layout",
  "icon": "index-card",
  "description": "A flexible container that can hold any blocks with customizable layout and styling.",
  "supports": {
    "html": false,
    "anchor": true,
    "align": [
      "wide",
      "full"
    ]
  },
  "attributes": {
    "justifyContent": {
      "type": "string",
      "default": "flex-start"
    },
    "alignItems": {
      "type": "string",
      "default": "stretch"
    },
    "flexDirection": {
      "type": "string",
      "default": "row"
    },
    "gap": {
      "type": "number",
      "default": 0
    },
    "flexDirectionMobile": {
      "type": "string",
      "default": "column"
    },
    "justifyContentMobile": {
      "type": "string",
      "default": "flex-start"
    },
    "alignItemsMobile": {
      "type": "string",
      "default": "stretch"
    },
    "gapMobile": {
      "type": "number",
      "default": 0
    },
    "flexDirectionTablet": {
      "type": "string",
      "default": "row"
    },
    "justifyContentTablet": {
      "type": "string",
      "default": "flex-start"
    },
    "alignItemsTablet": {
      "type": "string",
      "default": "stretch"
    },
    "gapTablet": {
      "type": "number",
      "default": 0
    },
    "containerWidth": {
      "type": "string",
      "default": "full"
    },
    "maxWidth": {
      "type": "number",
      "default": 1200
    },
    "paddingTop": {
      "type": "number",
      "default": 0
    },
    "paddingRight": {
      "type": "number",
      "default": 0
    },
    "paddingBottom": {
      "type": "number",
      "default": 0
    },
    "paddingLeft": {
      "type": "number",
      "default": 0
    },
    "marginTop": {
      "type": "number",
      "default": 0
    },
    "marginRight": {
      "type": "number",
      "default": 0
    },
    "marginBottom": {
      "type": "number",
      "default": 0
    },
    "marginLeft": {
      "type": "number",
      "default": 0
    },
    "backgroundColor": {
      "type": "string",
      "default": ""
    },
    "backgroundOpacity": {
      "type": "number",
      "default": 1
    },
    "backgroundGradient": {
      "type": "string",
      "default": ""
    },
    "backgroundImageURL": {
      "type": "string",
      "default": ""
    },
    "backgroundPosition": {
      "type": "string",
      "default": "center center"
    },
    "backgroundSize": {
      "type": "string",
      "default": "cover"
    },
    "backgroundRepeat": {
      "type": "string",
      "default": "no-repeat"
    },
    "overlayColor": {
      "type": "string",
      "default": ""
    },
    "overlayOpacity": {
      "type": "number",
      "default": 0
    },
    "borderStyle": {
      "type": "string",
      "default": "none"
    },
    "borderColor": {
      "type": "string",
      "default": ""
    },
    "borderWidth": {
      "type": "number",
      "default": 0
    },
    "borderRadius": {
      "type": "number",
      "default": 0
    }
  },
  "usesContext": [],
  "providesContext": {},
  "example": {},
  "editorScript": "file:./index.js",
  "viewScript": "file:./view.js",
  "style": "file:./style.scss",
  "editorStyle": "file:./editor.scss"
}
    "menuBorderRadius": {
      "type": "number",
      "default": 0
    },
    "showCanvasTitle": {
      "type": "boolean",
      "default": false
    },
    "fontSize": {
      "type": "number",
      "default": 16
    },
    "fontWeight": {
      "type": "string",
      "default": "400"
    },
    "boldWeight": {
      "type": "string",
      "default": "700"
    },
    "fontFamily": {
      "type": "string",
      "default": "inherit"
    },
    "underlineColor": {
      "type": "string",
      "default": "#000000"
    },
    "isSticky": {
      "type": "boolean",
      "default": true
    },
    "containerMode": {
      "type": "string",
      "default": "full"
    },
    "containerMaxWidth": {
      "type": "object",
      "default": {
        "desktop": {
          "value": 1200,
          "unit": "px"
        },
        "tablet": {
          "value": 100,
          "unit": "%"
        },
        "mobile": {
          "value": 100,
          "unit": "%"
        }
      }
    },
    "padding": {
      "type": "object",
      "default": {
        "top": 20,
        "right": 20,
        "bottom": 20,
        "left": 20
      }
    },
    "margin": {
      "type": "object",
      "default": {
        "top": 0,
        "right": 0,
        "bottom": 0,
        "left": 0
      }
    }
